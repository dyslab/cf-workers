{
  "name": "subs2bucket",
  "version": "0.0.6",
  "private": true,
  "scripts": {
    "deploy": "npm run version:patch && wrangler deploy",
    "dev": "wrangler dev --test-scheduled",
    "start": "wrangler dev --test-scheduled",
    "version:minor": "npm version minor --no-commit-hooks --no-git-tag-version",
    "version:patch": "npm version patch --no-commit-hooks --no-git-tag-version"
  },
  "devDependencies": {
    "wrangler": "^3.63.1"
  }
}
